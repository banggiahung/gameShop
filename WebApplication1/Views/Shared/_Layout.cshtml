<style>
    :root {
        --background-image: url('@ViewData["BackgroundImageUrl"]');
    }

    body {
        background-image: var(--background-image);
        overflow: hidden;
    }

    .show {
        width: 100% !important;
    }
    .scroll-wrapper {
        overflow: auto;
        max-height: 100vh;
        position: relative;
    }

        div.asp_w.asp_r .results::-webkit-scrollbar,
        .scroll-wrapper::-webkit-scrollbar {
            width: 8px;
        }

        div.asp_w.asp_r .results::-webkit-scrollbar-track,
        .scroll-wrapper::-webkit-scrollbar-track {
            background-color: rgba(0, 0, 0, 0.1);
