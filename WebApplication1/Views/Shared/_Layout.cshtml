                dataToolChoose: [],
                dataSubChoose: [],
                dataBank: [],
                dataBankChoose: {},
                categorySlect: "",
                inputSearch: ""

            },
            computed: {
                filteredDataItemsLayout() {
                    const searchResults = this.dataItemsLayout.filter((product) =>
                        product.name.toLowerCase().includes(this.searchKeywordPage.toLowerCase())
                    );

                    if (this.searchKeywordPage === '' || searchResults.length > 0) {
                        this.falseRs = true;
                        return searchResults;
                    } else {
                        this.falseRs = false;

                        return [{ msg: 'Không tìm thấy kết quả phù hợp' }];
                    }
                   
                },

                searchResultsStyle() {
                    const style = {};
                    const minHeight = 376;

                    if (this.dataItemsLayout.length < 5) {
                        style['height'] = 'auto';
                        style['overflow-y'] = 'auto';
                    } else if (this.searchKeywordPage === '') {
                        style['height'] = minHeight + 'px';
                        style['overflow-y'] = 'auto';
                    } else if (this.falseRs == false) {
                        style['height'] = 'auto';
                        style['overflow-y'] = 'auto';
                    }
                    else {
                        style['height'] = minHeight + 'px';
                        style['overflow-y'] = 'auto';
                    }
                    return style;
                },
                filteredBanks() {
                    return this.dataBank.filter(bank => bank.loaiBank === 'Chuyển khoản ngân hàng');
                },
                filteredBanks2() {
                    return this.dataBank.filter(bank => bank.loaiBank === 'Ví điện tử');
                }
            },
            beforeDestroy() {
                window.removeEventListener('click', this.handleWindowClick);
            },
            watch:{
                filteredDataItemsLayout(newVal) {
                    if (newVal.length === 1 && newVal[0].msg === 'Không tìm thấy kết quả phù hợp') {
                        this.shouldShowResults = false;

                    } else {
                        this.shouldShowResults = true;

                    }
                },
                user(valuePr) {
                    EventBus.$emit('userData', valuePr);
                }, 
                soDu(valuePr) {
                    EventBus.$emit('soDuUser', valuePr);
                }
            },
            mounted() {
                if (this.user.trim() !== '') {
                    this.userCheck = true;
                } else {
                    this.userCheck = false;

                }
                const token = localStorage.getItem('token');
                if(token)
                {
                    this.token = token;

                }
                else{
                    this.token = null;
                }
               
                axios.get("/Products/GetAllProduct")
                    .then((response) => {

                        this.dataItemsLayout = response.data;
                        return Promise.resolve();
                    });
                axios.get("/Products/GetAllCategory")
                    .then((response) => {

                        this.CateItemsLayout = response.data;
                        return Promise.resolve();
                    });
                axios.get("/Products/GetAllNoti")
                    .then((response) => {
                        this.notiData = response.data;
                        return Promise.resolve();
                    });
                window.addEventListener('click', this.handleWindowClick);
                this.listBank();

            },
            methods: {
                closeModal() {
                    $('#bankNapTien').modal('hide');
                    $('#bankForm').modal('show');

                },
                async clickBank(data) {
                    $('#bankForm').modal('hide');
                    $('#bankNapTien').modal('show');

                    this.dataBankChoose = data;
                },
                async confirmData() {
                    try {
                        if (this.PriceUser < 10000 || this.PriceUser == 0 ) {
                            Swal.fire({
                                icon: 'error',
                                title: 'Lỗi',
                                text: 'Tối thiểu nạp 10.000đ',
                                confirmButtonText: 'OK'
                            });
                            return;
                        }
                        $('#preloader').fadeIn();

                        const formData = new FormData();
                        formData.append('PriceUser', this.PriceUser);
                        formData.append('BankId', this.dataBankChoose.id);
                        formData.append('ContentTransit', '@ViewBag.userName');
                        await axios.post('/User/UpdateRechaUser', formData,
                            {
                                headers: {
                                    'Content-Type': 'multipart/form-data'
                                }
                            }).then(res => {
                                $('#preloader').fadeOut();

                                if (res.data.code == 200) {
                                    Swal.fire({
                                        icon: 'success',
                                        title: 'Thành công',
                                        text: 'Tiền sẽ được cộng trong ít phút',
                                        confirmButtonText: 'OK'
                                    }).then((result) => {
                                        if (result.isConfirmed) {
                                            window.location.reload();

                                        }
                                    });
                                }

                                else {
                                    Swal.fire({
                                        icon: 'error',
                                        title: 'Lỗi',
                                        text: `${res.data.message}`,
                                        confirmButtonText: 'OK'
                                    }).then((result) => {
                                        if (result.isConfirmed) {

                                            window.location.reload();


                                        }
                                    });
                                }

                            })


                    }
                    catch (error) {
                        console.error(error);
                        $('#preloader').fadeOut();

                        Swal.fire({
                            icon: 'error',
                            title: 'Lỗi',
                            text: 'Đã có lỗi xảy ra',
                            confirmButtonText: 'OK'
                        }).then((result) => {
                            if (result.isConfirmed) {
                                $('#bankNapTien').modal('hide');
                            }
                        });
                    }

                },
                listBank() {
                    $('#preloader').fadeIn();

                    axios.get(`/User/GetAllBank`)
                        .then((response) => {
                            $('#preloader').fadeOut();

                            this.dataBank = response.data;
                            return Promise.resolve();
                        });
                },
                removeToken(){
                    localStorage.removeItem('CheckLog')
                    localStorage.removeItem('token');
                    window.location.href = "/User/Logout";

                },
                async postLogin(){
                    const formData = new FormData();
                    formData.append("UserName", this.userName);
                    formData.append("Password", this.password);
                    await axios.post('/User/Login', formData,
                        {
                            headers: {
                                'Content-Type': 'multipart/form-data'
                            }
                        }).then(res => {
                            if (res.data.code == 400) {
                                Swal.fire({
                                    icon: 'warning',
                                    title: 'Lỗi',
                                    text: `${res.data.message}`,
                                    confirmButtonText: 'OK'
                                });
                                this.mess = res.data.message;
                            }
                            else {
                                axios.get('/get-id-user')
                                    .then(userResponse => {
                                        const token = userResponse.data;
                                        localStorage.setItem('token', token);
                                        localStorage.removeItem('CheckLog')

                                        Swal.fire({
                                            icon: 'success',
                                            title: 'Thành công',
                                            text: "Thành công",
                                            confirmButtonText: 'OK'
                                        }).then((result) => {
                                            if (result.isConfirmed) {
                                                window.location.href = "/";
                                            }
                                        });
                                    })
                                    .catch(error => {
                                        console.error("Error fetching user ID: ", error);
                                    });
                                
                            }


                        })
                },
                formatDate(date) {
                    const options = { year: 'numeric', month: '2-digit', day: '2-digit' };
                    return date.toLocaleDateString('vi-VN', options);
                },
                handleWindowClick(event) {

                    if (!this.$el.contains(event.target)) {
                        this.dropdownOpen = false;
                    }
                },
                toggleDropdown() {
                    this.dropdownOpen = !this.dropdownOpen;
                },
                handleSearch() {
                    this.dataItemsLayout;
                },
                showResults() {
                    this.showSearchResults = true;
                },
                hideResults() {
                    this.showSearchResults = false;
                },
                closeHandle() {
                    this.showSearchResults = false;
                    this.searchKeywordPage = ""

                },
                SearchClick(){
                    if (this.searchKeywordPage){
                        window.location.href = `/keywords/${this.searchKeywordPage}`
                    }
                    return;
                },
                formatCurrency(amount) {
                    const formatter = new Intl.NumberFormat('vi-VN', {
                        style: 'currency',
                        currency: 'VND'
                    });

                    return formatter.format(amount);
                },

            }
        });
    </script>
    <script>
        function toggleShow() {
            var el = document.getElementById("box");
            el.classList.toggle("show");
        }
        const mobileMenuToggle = document.getElementById('showMenu');
        const mobileMenu = document.getElementById('navbarToggleExternalContent');

        mobileMenuToggle.addEventListener('click', () => {
            if (mobileMenu.classList.contains('show')) {
                mobileMenu.classList.remove('show');
            } else {
                mobileMenu.classList.add('show');
            }
        });

    </script>
