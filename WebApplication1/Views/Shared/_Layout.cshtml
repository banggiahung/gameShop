    <script>
        search_vue = new Vue({
            el: '#search_vue',
            data: {
                dataItemsLayout: [],
                showSearchResults: false,
                searchKeywordPage: "",
                dropdownOpen: false,
                notiData: [],
                falseRs: false,
                CateItemsLayout: [],
                shouldShowResults: false,
