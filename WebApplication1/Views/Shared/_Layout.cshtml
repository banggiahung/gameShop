                background-color: rgba(0, 0, 0, 0.5);
            }

    .search__box {
        float: left;
        width: 0;
        height: 2rem;
        /*   display: inline; */
        background: none;
        color: #000;
        font-size: 1rem;
        border-radius: 2rem;
        outline: none;
        border: none;
        opacity: 1;
        transition: all .75s ease-in;
        cursor: pointer;
    }


    .search__icon {
        box-sizing: border-box;
        float: right;
        font-size: 1.5rem;
        display: inline-block;
        top: 0;
        margin-left: .8rem;
        margin-top: 0;
        cursor: pointer;
        position: absolute;
        color: #fa983a;
        transition: all .25s ease-in;
        padding: .7rem;
        border-radius: 50%;
